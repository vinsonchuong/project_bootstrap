#!/bin/bash -e
SHOW_HELP='no'
OUTPUT="$PWD"

ARGS=$(getopt -o  ho: -l help,output: -- "$@")
eval set -- $ARGS
while [ $# -gt 0 ]
do
	case "$1" in
		-h|--help)
			SHOW_HELP=yes
			shift
			;;
		-o|--output)
			OUTPUT="$2"
			shift 2
			;;
		--)
			shift
			break
			;;
	esac
done

if [ "$SHOW_HELP" = 'yes' ]
then
	cat '/usr/share/APP_NAME/help/APP_NAME'
else
	echo "$OUTPUT"
fi
